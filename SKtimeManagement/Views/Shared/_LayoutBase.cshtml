<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@(String.IsNullOrEmpty(ViewBag.Title) ? "SKtime Management" : ViewBag.Title)</title>
    @Styles.Render("~/Style/Base")
    @RenderSection("styles", required: false)
</head>
<body>
    @Scripts.Render("~/Script/Base")

    <div id="page-body">
        @RenderBody()
    </div>

    @RenderSection("scripts", required: false)
</body>
</html>