@model SKtimeManagement.ImportList
<div class="tabs">
    <div id="heads">
        <span class="active">Nhập kho</span>
        <a href="@Url.Action("Create", "Export")" target="_self" for="main-content" ptitle="Xuất kho">Xuất kho</a>
        <a href="@Url.Action("Create", "Order")" target="_self" for="main-content" ptitle="Hóa đơn">Hóa đơn</a>
    </div>
</div>
<div class="flexible">
    <div id="import-history" class="w25">
        @Html.Partial(Views.Filter, Model)
    </div>
    <div id="import-detail" class="w75 padding">
        <h3 class="text-center">Phiếu nhập kho</h3>
        @if (!String.IsNullOrEmpty(Model.Message))
        {
            <div class="text-center @(Model.Result ? "text-info" : "text-danger")">
                <i>@Model.Message</i>
            </div>
        }
        @Html.Partial(Views.Products, Model.Current)
    </div>
</div>