@model SKtimeManagement.ExportList
@{
    ViewBag.Title = "Xuất kho";
    Layout = "~/Views/Shared/_LayoutHome.cshtml";
}
<div class="tabs">
    <div id="heads">
        <a href="@Url.Action("Create", "Import")" target="_self" for="main-content" ptitle="Nhập kho">Nhập kho</a>
        <span class="active">Xuất kho</span>
        <a href="@Url.Action("Create", "Order")" target="_self" for="main-content" ptitle="Hóa đơn">Hóa đơn</a>
    </div>
</div>
<div class="flexible">
    <div id="export-history" class="w25">
        @Html.Partial(Views.Filter, Model)
    </div>
    <div id="export-detail" class="w75">
        @Html.Partial(Views.UpdatePartial, Model)
    </div>
</div>